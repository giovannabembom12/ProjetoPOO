package com.model.dao;

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.util.ArrayList;
import java.util.List;

import javax.swing.JOptionPane;
import com.model.vo.AssociadosVO;
import com.view.Associados;

public class AssociadosDAO {
	Connection conn;
	PreparedStatement pstm;

	public ResultSet autenticacaoAssociados(AssociadosVO objassociadosvo) {
		conn = new Conexao().conectaBD();
		return null;

	}

	public void cadastrarAssociados(AssociadosVO objassociadosvo2) {
		String sql = "INSERT INTO tbAssociados (nome, segmento, contato) VALUES (?, ?, ?)";

		conn = new Conexao().conectaBD();

		try {
			pstm = conn.prepareStatement(sql);
			pstm.setString(1, objassociadosvo2.getNome());
			pstm.setString(2, objassociadosvo2.getSegmento());
			pstm.setInt(3, objassociadosvo2.getContato());

			pstm.execute();
			pstm.close();

			JOptionPane.showMessageDialog(null, "Cadastro conclu√≠do com sucesso!");

		} catch (SQLException erro) {
			JOptionPane.showMessageDialog(null, "AssociadosDAO" + erro);
		}

	}

}

package com.model.vo;

public class AssociadosVO {
	private int id;
	private String nome;
	private String segmento;
	private int contato;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getSegmento() {
		return segmento;
	}
	public void setSegmento(String segmento) {
		this.segmento = segmento;
	}
	public int getContato() {
		return contato;
	}
	public void setContato(int contato) {
		this.contato = contato;
	}

}

