package com;
import java.awt.Desktop;
import java.io.File;
import java.io.IOException;
public class AbrirPDF {
	//oi bembom
public static void main(String[] args) {
String caminhoArquivo = "AQUI VC COPIA O CAMINHO DO PDF DO MANUAL DO USUÁRIO, qualquer coisa manda mensagem";
abrirPDF(caminhoArquivo);
}
public static void abrirPDF(String caminhoArquivo) {
try {
if (Desktop.isDesktopSupported()) {
Desktop desktop = Desktop.getDesktop();
File arquivoPDF = new File(caminhoArquivo);
if (arquivoPDF.exists()) {
desktop.open(arquivoPDF);
} else {
System.out.println("Arquivo não encontrado: " + caminhoArquivo);
}
} else {
System.out.println("puts");
}
} catch (IOException e) {
e.printStackTrace();
}
}
}
