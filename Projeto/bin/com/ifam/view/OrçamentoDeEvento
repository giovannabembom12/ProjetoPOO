    private void carregarDadosDoBanco() {
    	Connection conn = null;
    	try {
    	conn = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/tasktide?user=root&password=root");
    	String sql = "SELECT nome, dataprevis FROM tbEvento WHERE id = ?";
    	PreparedStatement stmt = conn.prepareStatement(sql);
    	stmt.setInt(1, 1);
    	ResultSet rs = stmt.executeQuery();
    	
    	
    	if (rs.next()) {
    	String nomeEvento = rs.getString("nome");
    	String dataEvento = rs.getString("dataprevis");
    	lblNomeDoEvento.setText(nomeEvento);
    	lblData.setText(dataEvento);
    	}
    	} catch (Exception e) {
    	e.printStackTrace();
    	JOptionPane.showMessageDialog(null, "Erro ao carregar dados do banco de dados: " + e.getMessage());
    	} finally {
    	try {
    	if (conn != null) {
    	conn.close();
    	}
    	} catch (Exception e) {
    	e.printStackTrace();
    	}
    	}
    	}
    
    private void carregarDadosCliente() {
        Connection conn = null;
        try {
            conn = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/tasktide?user=root&password=root");
            String sql = "SELECT nome, telefone FROM tbCliente WHERE id = ?";
            PreparedStatement stmt = conn.prepareStatement(sql);
            stmt.setInt(1, 1); 
            ResultSet rs = stmt.executeQuery();
            if (rs.next()) {
                String nomeCliente = rs.getString("nome");
                String telefoneCliente = rs.getString("telefone");

                lblNomeDoCliente.setText(nomeCliente);
                lblTelefone.setText(telefoneCliente);
            }
        } catch (Exception e) {
            e.printStackTrace();
            JOptionPane.showMessageDialog(null, "Erro ao carregar dados do cliente: " + e.getMessage());
        } finally {
            try {
                if (conn != null) {
                    conn.close();
                }
            } catch (Exception e) {
                e.printStackTrace();
            }
        }
    }

    
    private void Calcular() {
   	 Connection conn = null;
   	 try {
   		 conn = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/tasktide?user=root&password=root");
   		 String sql = "SELECT SUM(valor_total) AS total FROM tbDados";
   		 PreparedStatement stmt = conn.prepareStatement(sql);
   		 ResultSet rs = stmt.executeQuery();
   	if (rs.next()) {
   		int total = rs.getInt("total");
   		lblResposta.setText("Total: R$" + total);
   	}
   	 } catch (Exception e) {
   		 e.printStackTrace();
   		 JOptionPane.showMessageDialog(null, "Erro ao carregar dados do banco de dados: " + e.getMessage());
   } finally {
	   	try {
	   		if (conn != null) {
	   			conn.close();
   }
   } catch (Exception e) {
	   e.printStackTrace();
   }
   }
   }
    
    private void carregarDadosProduto1() {
        Connection conn = null;
        try {
            conn = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/tasktide?user=root&password=root");
            String sql = "SELECT quantidade, descricao, valor, valor_total FROM tbDados WHERE descricao = ?";
            PreparedStatement stmt = conn.prepareStatement(sql);
            stmt.setString(1, "produto 1");
            ResultSet rs = stmt.executeQuery();
            if (rs.next()) {
                int quantidade = rs.getInt("quantidade");
                String descricao = rs.getString("descricao");
                int valor = rs.getInt("valor");
                int valorTotal = rs.getInt("valor_total");
                
                lblQuantidade1.setText("Quantidade: " + quantidade);
                lblDescricao1.setText("Descrição: " + descricao);
                lblValor1.setText("Valor: " + valor);
                lblValorTotal1.setText("Valor Total: " + valorTotal);
            }
        } catch (Exception e) {
            e.printStackTrace();
            JOptionPane.showMessageDialog(null, "Erro ao carregar dados do banco de dados: " + e.getMessage());
        } finally {
            try {
                if (conn != null) {
                    conn.close();
                }
            } catch (Exception e) {
                e.printStackTrace();
            }
        }
    }

    private void carregarDadosProduto2() {
        Connection conn = null;
        try {
            conn = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/tasktide?user=root&password=root");
            String sql = "SELECT quantidade, descricao, valor, valor_total FROM tbDados WHERE descricao = ?";
            PreparedStatement stmt = conn.prepareStatement(sql);
            stmt.setString(1, "produto 2");
            ResultSet rs = stmt.executeQuery();
            if (rs.next()) {
                int quantidade = rs.getInt("quantidade");
                String descricao = rs.getString("descricao");
                int valor = rs.getInt("valor");
                int valorTotal = rs.getInt("valor_total");
                
                lblQuantidade2.setText("Quantidade: " + quantidade);
                lblDescricao2.setText("Descrição: " + descricao);
                lblValor2.setText("Valor: " + valor);
                lblValorTotal2.setText("Valor Total: " + valorTotal);
                //lblValorTotal2.setText(String.valueOf(valorTotal));

            }
        } catch (Exception e) {
            e.printStackTrace();
            JOptionPane.showMessageDialog(null, "Erro ao carregar dados do banco de dados: " + e.getMessage());
        } finally {
            try {
                if (conn != null) {
                    conn.close();
                }
            } catch (Exception e) {
                e.printStackTrace();
            }
        }
    }

    
    
